    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.0.0",
    "@typescript-eslint/parser": "5.0.0",
    "concurrently": "6.5.1",
    "eslint": "8.0.1",
    "fuegojs": "^0.25.8",
    "prettier": "2.4.1",
    "tailwindcss": "^3.0.24",
    "ts-node": "10.3.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-react-hooks": "2.2.2",
    "typescript": "4.5.2"
  },
  "dependencies": {
